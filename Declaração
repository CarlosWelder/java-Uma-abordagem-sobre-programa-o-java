package capituloII;


public class Declaracao {
    public static void main(String[] args) {
    
        int num;
        num = 0;
        int numero = 0;

        char letra = 'F';

        boolean restituicao = true;
        if (restituicao) {
            System.out.println("Receber restituição");
            
        }
        else{
            System.out.println("Não receber restituição");
        }
    }

}
